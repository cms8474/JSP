<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>반복문</title>
		<%--
		2507028
		최민수
		JSP 스크립트릿 반복문 실습
		 --%>
	</head>
	<body>
		<h3>반복문</h3>
		
		<h4>for</h4>
		<%
			for(int i=1 ; i<=5 ; i++){
				out.println("<p>i : " + i + "</p>");
			}
		
		%>
		
		<% for(int j=1 ; j<=5 ; j++){ %>
				<p>j : <%= j %></p>
		<%}%>
		
		
		
	</body>
</html>